{
    "name": "backend",
    "description": "API For Social Media Aggregator",
    "keywords": ["framework", "laravel", "lumen", "api"],
    "license": "aGPL-v3",
    "type": "project",
    "require": {
        "php": ">=5.6.4",
        "laravel/lumen-framework": "5.4.*",
        "vlucas/phpdotenv": "~2.2",
        "abraham/twitteroauth": "^0.7.4",
        "socialiteproviders/google": "^3.0",
        "socialiteproviders/twitter": "^3.0",
        "socialiteproviders/youtube": "^3.0",
        "madcoda/php-youtube-api": "^1.2",
        "vimeo/vimeo-api": "^1.2"
    },
    "require-dev": {
        "wn/lumen-generators": "^1.3",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "~0.9",
        "laravel/tinker": "^1.0@dev"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/",
            "database/"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
